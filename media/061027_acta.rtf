\par \pard\plain \ltrpar\s1\cf0{\*\hyphen2\hyphlead2\hyphtrail2\hyphmax0}\ql\rtlch\af12\afs24\lang255\ltrch\dbch\af9\langfe255\hich\f1\fs24\lang1034\loch\f1\fs24\lang1034 {\rtlch \ltrch\loch\f1\fs24\lang1034\i0\b0 Viernes 27-oct, 16:30 - Esc. Ing. San Mam\'e9s, Bilbao}
